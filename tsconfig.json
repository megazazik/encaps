{
	"compilerOptions": {
		"sourceMap": true,
		"module": "commonjs",
		"target": "es5",
		"declaration": true,
		"outDir": "./dist/",
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"allowUnreachableCode": false,
		"importHelpers": true,
		"jsx": "react",

	},
	"include": [
        "src/**/*"
    ]
}